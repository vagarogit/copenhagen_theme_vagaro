    "prettier": "2.8.4",
    "puppeteer": "19.4.1",
    "rollup": "3.17.3",
    "rollup-jest": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "1.12.18",
    "sass": "1.58.3",
    "semantic-release": "19.0.5",
    "ts-jest": "^29.2.4",
    "typescript": "^5.1.6",
    "url-pattern": "1.0.3",
    "vinyl": "^3.0.0",
    "vinyl-fs": "^4.0.0",
    "wait-on": "7.0.1"
  },
  "resolutions": {
    "@types/react": "^17.x"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
